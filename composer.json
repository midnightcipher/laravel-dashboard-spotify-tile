{
    "name": "ashbakernz/laravel-dashboard-spotify-tile",
    "description": "Displays data from spotfiy on a tile for spatie/laravel-dashboard",
    "keywords": [
        "ashbakernz",
        "laravel-dashboard-spotify-tile"
    ],
    "homepage": "https://github.com/ashbakernz/laravel-dashboard-spotify-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "ashbakernz",
            "email": "ashbakernz@gmail.com",
            "homepage": "https://ashbaker.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ashbakernz\\SpotifyTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ashbakernz\\SpotifyTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ashbakernz\\SpotifyTile\\SpotifyTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
